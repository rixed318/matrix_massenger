{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Matrix Messenger",
  "version": "0.1.0",
  "identifier": "com.matrix.messenger.dev",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Matrix Messenger",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
